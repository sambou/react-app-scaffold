{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Frontend scaffold based on node.js/express server, webpack build tool, React as view library ...",
  "repository": "https://github.com/sambou/react-boilerplate",
  "main": "index.js",
  "scripts": {
    "prestart": "rm -rf target",
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./src/server/index.ts & ./node_modules/.bin/webpack --watch --config ./webpack/webpack.development.config.js",
    "test": "./node_modules/.bin/mocha --require ts-node/register test/**/*.ts*",
    "tdd": "./node_modules/.bin/mocha --require ts-node/register test/**/*.ts* --watch",
    "postinstall": "bash bin/build.sh",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p --config ./webpack/webpack.production.config.js --optimize-minimize"
  },
  "author": "Sami Boussaid <sboussaid3@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "1.2.1",
    "assertion-error": "1.0.2",
    "babel": "6.5.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-regenerator": "6.9.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-3": "6.11.0",
    "babel-register": "6.9.0",
    "babel-runtime": "6.9.2",
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "^0.6.5",
    "jsdom": "9.4.1",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "react": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "redbox-react": "1.2.10",
    "redux": "3.5.2",
    "redux-devtools": "3.3.1",
    "redux-thunk": "2.1.0",
    "sass-loader": "4.0.0",
    "style-loader": "^0.13.0",
    "ts-loader": "0.8.2",
    "ts-node": "0.9.3",
    "typescript": "1.8.10",
    "typings": "1.3.1",
    "webpack": "1.13.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-manifest-plugin": "1.0.1",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "2.0.2",
    "eventsource-polyfill": "0.9.6",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.3",
    "redux-devtools": "3.2.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
