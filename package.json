{
  "name": "frontend-shell-vanilla",
  "version": "0.0.1",
  "description": "Frontend shell based on node.js/express server, webpack build tool, React as view library ...",
  "main": "index.js",
  "scripts": {
    "prestart": "rm -rf target",
    "start": "NODE_ENV=development node index.js & ./node_modules/.bin/webpack --watch",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register",
    "postinstall": "bash bin/build.sh",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p --config webpack.production.config.js --optimize-minimize"
  },
  "author": "Sami Boussaid <sboussaid3@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^1.17.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^1.7.0",
    "image-webpack-loader": "^1.6.2",
    "node-sass": "^3.4.2",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.10"
  },
  "devDependencies": {
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "redux-devtools": "^3.0.1"
  }
}
