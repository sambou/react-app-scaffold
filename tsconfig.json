{
  "compileOnSave": false,
  "filesGlob": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./test/**/*.ts",
    "./test/**/*.tsx",
    "!./node_modules/**/*.ts",
    "./typings/tsd.d.ts"
  ],
  "compilerOptions": {
    "jsx": "react",
    "noEmitOnError": true,
    "module": "commonjs",
    "sourceMap": true,
    "target": "es6"
  },
  "files": [
    "./src/actions/action.ts",
    "./src/actions/index.ts",
    "./src/middleware/logger.ts",
    "./src/reducers/index.ts",
    "./src/store/index.ts",
    "./src/components/sample-page.tsx",
    "./src/containers/app.tsx",
    "./src/index.tsx",
    "./test/components/sample-page.tsx",
    "./typings/tsd.d.ts"
  ],
  "exclude": [],
  "atom": {
    "rewriteTsconfig": true
  }
}
